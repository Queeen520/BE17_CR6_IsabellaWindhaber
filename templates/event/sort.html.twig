<div class="container">
  {% for i in music %}
  <div class="row-cols-3 card border-primary mb-3" style="max-width: 18rem">
    <div class="card">
      <img src="{{ i.img }}" class="card-img-top" alt="..." />
      <div class="card-header">{{ i.type }}</div>
      <div class="card-body text-primary">
        <h5 class="card-title">{{ i.name }}</h5>
      </div>
    </div>
  </div>
  {% endfor %}
</div>

